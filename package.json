{
  "name": "rinocloud-watcher",
  "version": "0.0.1",
  "description": "Rinocloud application to watch folders",
  "scripts": {
    "test": "babel-node --presets es2015 ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rinocloud/watcher.git"
  },
  "keywords": [
    "rinocloud"
  ],
  "author": "Rinocloud",
  "license": "",
  "bugs": {
    "url": "https://github.com/rinocloud/watcher/issues"
  },
  "homepage": "https://github.com/rinocloud/watcher#readme",
  "devDependencies": {
    "es6-promise": "^3.2.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "evaporate": "git+https://github.com/TTLabs/EvaporateJS.git",
    "superagent": "^1.8.3",
    "superagent-promise-plugin": "^3.2.0",
    "superagent-queue": "0.0.3"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "lodash": "^4.6.1",
    "webpack": "^1.12.6",
    "webpack-bundle-tracker": "0.0.8"
  },
  "eslintConfig": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "extends": "airbnb",
    "ecmaFeatures": {
      "jsx": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "no-console": [
        2,
        {
          "allow": [
            "log",
            "warn",
            "error"
          ]
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  }
}
